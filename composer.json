    "name": "bruninoit/abc",
    "type": "phpbb-extension",
    "description": "Adds a list with a number of recent topics to the index.php",
    "homepage": "https://github.com/bruninoit/adsenseblock",
    "version": "0.1.0-a1",
    "time": "2015-06-14",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "brunino",
            "email": "brunino@altervista.org",
            "homepage": "http://www.brunino.altervista.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "extra": {
        "display-name": "Cookie Adsense Block",
        "soft-require": {
             "phpbb/phpbb": "3.1.*@dev"
        },
        "version-check": {
            "host": "brunino.altervista.org",
            "directory": "/version/",
